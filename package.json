  },
  "homepage": "https://github.com/GuilhermeSantanaAndrade/POC_express_mocha_chai#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.4.0",
    "moment": "^2.24.0",
    "sqlite3": "^4.1.1",
    "uuid": "^7.0.3"
